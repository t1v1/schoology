{
  "manifest_version": 2,
  "name": "Schoology",
  "version": "0.0",
  "description": "Firefox extension and web app to hack Schoology Learnosity assessments.",
  "homepage_url": "https://github.com/t1v1/schoology#readme",
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  },
  "content_scripts": [
    {
      "matches": ["https://pausd.schoology.com/common-assessment-delivery/*"],
      "js": ["schoology.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icons/48.png"
  }
}
